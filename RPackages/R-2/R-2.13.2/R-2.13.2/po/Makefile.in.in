	if test -n "$${trs}"; then \
	  for tr in $${trs}; do \
	    lang=`echo $${tr} | sed -e 's/^RGui-//' -e 's/\.po$$//'`; \
	    out=`echo $${tr} | sed -e 's/\.po$$/.gmo/'`; \
	    echo $(ECHO_N) "  $$lang:$(ECHO_C)"; \
	    (cd $(srcdir) && $(MSGMERGE_UPDATE) $${tr} RGui.pot); \
	    (cd $(srcdir) && $(MSGFMT) -c --statistics -o $${out} $${tr}) ; \
	  done; \
	fi




force:

