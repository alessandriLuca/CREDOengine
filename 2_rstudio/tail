
COPY rstudioServer /rstudioServer
RUN cd /rstudioServer/ && 7za -y x "*.7z*"
RUN apt-get install -y psmisc libapparmor1 sudo libclang-dev libpq5 gdebi-core
RUN gdebi /rstudioServer/rstudio-server-2021.09.1-372-amd64.deb
ENV USER=rstudio
